#import "LYCFactory.h"

@implementation LYCFactory


#pragma mark -- myProgressHud
+(void) showLycHudToObject:(UIView *)obj withTitil:(NSString *)title{
    MBProgressHUD *hud = [MBProgressHUD showHUDAddedTo:obj animated:YES];
    hud.mode = MBProgressHUDModeText;
    hud.label.text = title;
    [hud hideAnimated:YES afterDelay: 1];
}

@end
