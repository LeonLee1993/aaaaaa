}


- (void)setUpScrollView{
    
    
    UILabel *titleLabel = [[UILabel alloc]initWithFrame:CGRectMake(SCREEN_WIDTH/2-100, 17, 200, 15)];
    titleLabel.text = @"股博士软件服务协议";
    titleLabel.font = [UIFont systemFontOfSize:15];
    titleLabel.textAlignment = NSTextAlignmentCenter;
    titleLabel.textColor = titleColor3;
    [_protocalScrollView addSubview:titleLabel];
    
    
    NSMutableParagraphStyle *style1 = [[NSMutableParagraphStyle alloc] init];
    style1.headIndent = 0;
    style1.firstLineHeadIndent = 0;
    style1.lineSpacing = 5;
    
    
    UILabel * descLabel = [[UILabel alloc]initWithFrame:CGRectMake(10, CGRectGetMaxY(titleLabel.frame)+25, SCREEN_WIDTH-20, SCREEN_HEIGHT)];
    descLabel.numberOfLines = 0;
    NSMutableAttributedString *attriText =[[NSMutableAttributedString alloc]initWithString:descStr attributes:@{NSFontAttributeName:[UIFont systemFontOfSize:13],NSParagraphStyleAttributeName:style1,NSForegroundColorAttributeName:RGBCOLOR(102, 102, 102)}];
    [attriText setAttributes:@{NSFontAttributeName:[UIFont systemFontOfSize:13],NSParagraphStyleAttributeName:style1,NSForegroundColorAttributeName:titleColor3} range:NSMakeRange(descStr.length-33, 33)];
    descLabel.attributedText = attriText;
    
    [_protocalScrollView addSubview:descLabel];
    
    
    CGSize secondDesc = [descStr boundingRectWithSize:CGSizeMake(SCREEN_WIDTH-20, MAXFLOAT) options:NSStringDrawingUsesLineFragmentOrigin attributes:@{NSFontAttributeName:[UIFont systemFontOfSize:13],NSParagraphStyleAttributeName:style1} context:nil].size;
    
    
    _protocalScrollView.contentSize = CGSizeMake(SCREEN_WIDTH, secondDesc.height + 25+19+15);
    CGRect tempRect= descLabel.frame;
    tempRect.size.height = secondDesc.height;
    descLabel.frame = tempRect;
    
}

