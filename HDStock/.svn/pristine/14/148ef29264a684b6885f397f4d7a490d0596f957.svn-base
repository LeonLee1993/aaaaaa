#import "HDTopScrollCell.h"
#import "ATCarouselView.h"

@interface HDTopScrollCell()<ATCarouselViewDelegate>

@property (weak, nonatomic) IBOutlet ATCarouselView *topScrollView;

@end

@implementation HDTopScrollCell

- (void)awakeFromNib {
    [super awakeFromNib];
    // Initialization code
    
    self.topScrollView.delegate = self;
    
    self.topScrollView.images = @[imageNamed(@"jgqm"),imageNamed(@"jskpq"),imageNamed(@"banner"),imageNamed(@"big_news")];
    
    self.topScrollView.currentPageColor = [UIColor orangeColor];
    self.topScrollView.pageColor = [UIColor grayColor];
    
}

- (void)setSelected:(BOOL)selected animated:(BOOL)animated {
    [super setSelected:selected animated:animated];

    // Configure the view for the selected state
}

- (void)carouselView:(ATCarouselView *)carouselView indexOfClickedImageBtn:(NSUInteger)index{

    NSLog(@"第%lu张图片被点击",(unsigned long)index);

}

@end
