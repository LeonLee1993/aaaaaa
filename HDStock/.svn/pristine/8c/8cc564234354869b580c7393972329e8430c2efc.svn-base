#import <Foundation/Foundation.h>

@interface HDAdvertisementModel : NSObject

@property (nonatomic, assign) NSInteger advid;//广告id
@property (nonatomic, assign) NSInteger available;//是否启用
@property (nonatomic, copy) NSString * type;//类型
@property (nonatomic, assign) NSInteger displayorder;//显示顺序
@property (nonatomic, copy) NSString * title;//广告标题
@property (nonatomic, copy) NSString * link;//广告跳转地址
@property (nonatomic, copy) NSString * alt;//说明
@property (nonatomic, assign) NSInteger width;//广告宽度
@property (nonatomic, assign) NSInteger height;	//广告高度
@property (nonatomic, copy) NSString * url;	//广告地址
@property (nonatomic, copy) NSString * starttime;//开始时间

@property (nonatomic, strong) UIImage * bannerImage;
    
@end
