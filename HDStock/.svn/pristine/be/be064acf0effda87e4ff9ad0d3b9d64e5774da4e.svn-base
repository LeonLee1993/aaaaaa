- (UITableViewCell *)tableView:(UITableView *)tableView cellForRowAtIndexPath:(NSIndexPath *)indexPath{
    
    UITableViewCell * cell = [UITableViewCell new];
    
    HDAskStockTableViewCell * askCell = [self.tableview dequeueReusableCellWithIdentifier:@"HDAskStockTableViewCell"];
    HDAskPeepTableViewCell * peepCell = [self.tableview dequeueReusableCellWithIdentifier:@"HDAskPeepTableViewCell"];
    
    if (indexPath.row == 0) {
        cell = askCell;
    }else{
        
        cell = peepCell;
    }
    
    
    return cell;
    
}

- (NSInteger)tableView:(UITableView *)tableView numberOfRowsInSection:(NSInteger)section{
    
    return 4;
    
}

- (CGFloat)tableView:(UITableView *)tableView heightForRowAtIndexPath:(NSIndexPath *)indexPath{
    
    self.tableview.rowHeight = UITableViewAutomaticDimension;
    self.tableview.estimatedRowHeight = 200;
    return self.tableview.rowHeight;
    
}
