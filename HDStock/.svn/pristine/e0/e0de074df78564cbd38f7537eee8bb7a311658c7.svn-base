#import "PSYRefreshGifHeader.h"

@implementation PSYRefreshGifHeader

- (void)prepare{

    [super prepare];
    
    NSArray * idleImages = @[imageNamed(@"LOGO"),imageNamed(@"LOGO2"),imageNamed(@"LOGO3"),imageNamed(@"LOGO4"),imageNamed(@"LOGO5"),];
    [self setImages:idleImages forState:MJRefreshStateIdle];
    [self setImages:idleImages forState:MJRefreshStateRefreshing];
    [self setImages:idleImages forState:MJRefreshStatePulling];

    [self setImages:idleImages duration:0.2f forState:MJRefreshStateIdle];
    [self setImages:idleImages duration:0.2f forState:MJRefreshStateRefreshing];
    [self setImages:idleImages duration:0.2f forState:MJRefreshStatePulling];
    
}

@end
