#import <Foundation/Foundation.h>

@interface HDNewsCommentsModel : NSObject
@property (nonatomic,assign) NSInteger cid;	//评论id
@property (nonatomic,assign) NSInteger uid;		//发表评论的用户id
@property (nonatomic,copy)   NSString * username;	//发表评论的用户名
@property (nonatomic,assign) NSInteger id;	//评论对象id
@property (nonatomic,assign) NSInteger idtype;	//评论对象的id类型：aid    <topicid  暂不开放>
@property (nonatomic,assign) NSInteger postip;		//评论Ip
@property (nonatomic,assign) NSInteger port;	//请求端口
@property (nonatomic,copy)   NSString * dateline;	//评论时间戳
@property (nonatomic,assign) NSInteger status;	//评论状态： 1-审核
@property (nonatomic,copy)   NSString * message	;//评论内容

@end
